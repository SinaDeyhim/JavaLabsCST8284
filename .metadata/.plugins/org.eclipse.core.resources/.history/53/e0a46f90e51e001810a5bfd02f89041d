package test.cst8284.calculator;
import cst8284.calculator.*;

import static org.junit.Assert.*;

import org.junit.Test;

public class TestComplexCalculator {

	@Test
	void testToString() {
		Complex c = new Complex(3, 2);
		assertTrue(c.toString().equals("3.0 + 2.0i"));
	}

	@Test
	void testDivide() {
		String [] c= {"7","12i"};
		Complex c1 = new Complex(c);
		Complex c2 = new Complex("4","-4i");
		ComplexCalculator c3= new ComplexCalculator();
		assertTrue(c3.divide(c1,c2).equals("-0.625 + 2.375i"));
	}
	


}
